{
  "name": "@stripe/ai-sdk-provider",
  "version": "0.1.0",
  "description": "Custom Vercel AI SDK provider for Stripe's llm.stripe.com proxy with automatic usage tracking",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "stripe",
    "ai",
    "provider",
    "ai-sdk",
    "vercel"
  ],
  "author": "Stripe <support@stripe.com> (https://stripe.com/)",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "ai": "^5.0.76",
    "dotenv": "^17.2.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@ai-sdk/provider": "^2.0.0",
    "@ai-sdk/provider-utils": "^2.0.0",
    "stripe": "^17.5.0",
    "zod": "^3.24.1"
  },
  "peerDependencies": {
    "ai": "^3.4.7 || ^4.0.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "ai": {
      "optional": true
    }
  }
}
